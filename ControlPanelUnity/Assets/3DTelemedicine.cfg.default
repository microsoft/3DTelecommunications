[3DTMLauncherService]
#Verbosity level.  0 is default, minimal output. 1 will produce much more output in the event log.  2 and higher are extremely verbose and should only be used for debugging
Verbosity=1
DisableHeartbeat = false
BinaryLocation=C:\3DTelemedicine\3DTMLauncherService
# We can't overwrite this binary directly because it's the binary used to download updates, so it has to copy data somewhere and then
# the 3DTM Launcher Service Updater.exe will be called which will grab this value and overwrite the launcher
UpdatedZipFilename=C:\3DTelemedicine\3DTMLauncherService.new.zip
# When we've received a new binary and saved it to the filename above, 3DTMLauncherService will call [UpdaterExecutable]
# so [UpdaterExecutable] should find the new zip, stop the 3DTMLauncherService, overwrite files, restart 3DTMLauncherService
# and then exit
UpdaterExecutable=C:\3DTelemedicine\Tools\3DTM Launcher Service Updater.exe

